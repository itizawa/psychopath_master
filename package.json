{
  "name": "psychopath_master",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "yarn format-and-lint",
    "lint:fix": "yarn format-and-lint:fix",
    "format-and-lint": "biome check .",
    "format-and-lint:fix": "biome check . --write",
    "schema:format": "yarn prisma-case-format --file=./prisma/schema.prisma --config-file=./prisma/.prisma-case-format",
    "schema:gen": "yarn schema:format &&  yarn prisma generate --schema=./prisma/schema.prisma && yarn format-and-lint:fix",
    "migrate:dev": "yarn schema:format &&  yarn prisma migrate deploy --schema=./prisma/schema.prisma",
    "migrate:dev:reset": "yarn schema:format &&  yarn prisma migrate reset --skip-generate --force --schema=./prisma/schema.prisma",
    "migrate:gen": "yarn schema:gen && yarn prisma migrate dev --create-only --schema=./prisma/schema.prisma --name"
  },
  "dependencies": {
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.0.14",
    "@hookform/resolvers": "^3.9.0",
    "@mui/icons-material": "^6.0.0",
    "@mui/lab": "^5.0.0-alpha.173",
    "@mui/material": "^6.0.0",
    "@mui/material-nextjs": "^6.0.0",
    "@prisma/client": "5.21.1",
    "@tanstack/react-query": "^5.59.18",
    "@trpc/client": "^11.0.0-rc.608",
    "@trpc/react-query": "^11.0.0-rc.608",
    "@trpc/server": "^11.0.0-rc.608",
    "client-only": "^0.0.1",
    "next": "15.0.2",
    "notistack": "^3.0.1",
    "react": "19.0.0-rc-fb9a90fa48-20240614",
    "react-dom": "19.0.0-rc-fb9a90fa48-20240614",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "swr": "^2.2.5",
    "url-join": "^5.0.0",
    "zod": "^3.23.8",
    "zod-prisma-types": "^3.1.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/node": "^22.0.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "prisma": "^5.21.1",
    "prisma-case-format": "2.2.1",
    "typescript": "^5"
  }
}
